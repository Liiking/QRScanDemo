<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <size android:height="100dp" android:width="200dp"/>
            <solid android:color="@android:color/holo_green_dark" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>
    <item android:left="-10dp" android:bottom="90dp" android:right="190dp" android:top="-10dp">
        <shape android:shape="oval">
            <size android:height="20dp" android:width="20dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:left="-10dp" android:bottom="-10dp" android:right="190dp" android:top="90dp">
        <shape android:shape="oval">
            <size android:height="20dp" android:width="20dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>